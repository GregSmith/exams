<!DOCTYPE html>
<html>
<head>
  <title>Exams</title>
  <%= stylesheet_link_tag "scaffold" %>
  <%= stylesheet_link_tag "exams", :media => "all" %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<%= render "layouts/header" %>
  <div id="columns">
    <div id="side">
      <% if @cart %>
        <%= hidden_div_if(@cart.line_items.empty?, :id => 'cart') do %>
          <%= render @cart %>        
        <% end %>
      <% end %>
      <%= link_to 'Home', store_path %><br />
      <a href="http://www.">Questions</a><br />
      <a href="http://www.">About</a><br />
      <a href="http://www.">LSAT Prep Courses Online</a><br />
    </div>
    <div id="main">
      <% if notice %>
        <p id="notice"><%= notice %></p>
      <% end %>
      <%= yield %>
    </div>
    <% if @status %>
    <p>there's a status</p>
    <% end %>
    <% if @cart %>
    <p>there's a cart</p>
    <% end %>
    <% if @transaction_id %>
    <p>there's a transactionid</p>
    <% end %>
    <% if @cart_id %>
    <p>there's a cart id</p>
    <% end %>
    <% if @testing %>
    <p>there's a test </p>
    <% end %>
    <%= debug @cart %>

  </div>
</body>
</html>
